.runPanelNavigator{
	width: 100%;
    display: inline-flex;
    margin-top: 2%;
    color: #999999;
}

.line{
	background-color: #d6d6d6;
    display: block;
    height: 1px;
    width: 100%;
    position: relative;
    top: 11%;
    z-index: -1;
}

.stepWraper{
	display:flex;
	justify-content:center;
	align-items:center;
	width:25%;
	height:20%;
}

.stepWraperActive{
	font-weight: bolder;
}

.stepCount{
	width: 30px;
	height: 30px;
	border: 1px solid #999999;
	border-radius: 100px;
	background-color: #FFF;
	text-align:center;
	line-height:30px;
	margin-left: auto;
    margin-right: auto;
}

.stepCountActive{
	width: 30px;
	height: 30px;
	border: 1px solid #999999;
	border-radius: 100px;
	background-color: #999999;
	color: #FFF !important;
	text-align:center;
	line-height:30px;
	margin-left: auto;
    margin-right: auto;
}

.stepCountLabel{
	display: flex;
    justify-content: center;
    align-items: center;
}

.runPanelBody{
    height: 72%;
    align-content: center;
    display: flex;
    align-items: center;
    width: 100%;
}

.inputWrapers{
	display: inline-flex;
	margin-top: 2px;
    margin-left: 15px;
}

.inputSelector{
	width: 100px;
    height: 50px;
}

.submitbtn{
	    margin-left: 43%;
}