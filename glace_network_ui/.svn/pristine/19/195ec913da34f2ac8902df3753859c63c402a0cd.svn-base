<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources"
		type="com.glenwood.network.client.resource.DesktopResources" />
	<ui:style>
	.fontstyle {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel styleName='{resources.desktopstyles.pageloadFadeIn}'
		ui:field="AVPMainPanel">
		
		<g:FlowPanel >
			<g:FlowPanel>
				<g:Label>Add Node</g:Label>
				<g:Label>&#xe602;</g:Label>
			</g:FlowPanel>
			<g:ScrollPanel alwaysShowScrollBars="false" >
				<g:FlowPanel height="500px">
					<g:RadioButton text="Manual upload" name="uploadChoice"/>
					<g:RadioButton text="Upload using file" name="uploadChoice"/>
					<g:FlowPanel >
						<g:Label text="Network Name"/>
						<g:TextBox text="Enter Network Name"/>
					</g:FlowPanel>
					<g:FlowPanel >
						<g:Label text="Network Name"/>
						<g:TextBox text="Enter Network Name"/>
					</g:FlowPanel>
					<g:Button text="Add"/>
				</g:FlowPanel>
			</g:ScrollPanel>
		</g:FlowPanel>
		
		<g:Label styleName="{style.fontstyle}">Hello AVP Desktop test page 222</g:Label>
	</g:HTMLPanel>



</ui:UiBinder>