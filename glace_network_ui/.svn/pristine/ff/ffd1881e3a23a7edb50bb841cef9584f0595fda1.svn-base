.dragHistoryTable {
	border: 1px solid #CCCCCC;
	border-collapse: collapse;
	border-spacing: 0px;
	padding: 0px;
	height: 100%;
	width: 100%;
}

.dragHistoryTable tr:nth-of-type\(odd\) {
    background-color: #CCCCCC;
}
.dragHistoryTable tr:nth-of-type\(even\) {
    background-color: #FFFFFF;
}

.dragHistoryTable td {
	-moz-border-bottom-colors: none;
	-moz-border-left-colors: none;
	-moz-border-right-colors: none;
	-moz-border-top-colors: none;
	border-color: #CCCCCC;
	border-image: none;
	border-style: solid;
	border-width: 0px 1px 1px 0px;
	color: #000000;
	text-align: center;
	vertical-align: middle;
	height: 35px;
	/* width: 100px; */
}

.dragHistoryTable tr:firstChild td {
	-moz-border-bottom-colors: none;
	-moz-border-left-colors: none;
	-moz-border-right-colors: none;
	-moz-border-top-colors: none;
	background-color: #119DC9;
	border-color: #CCCCCC;
	border-image: none;
	border-style: solid;
	border-width: 0px 0px 1px 1px;
	color: #FFFFFF;
	font-size: 16px;
	font-weight: bold;
	height: 25px;
	text-align: center;
}

.dragHistoryTableWraper{
	display: block;
	width: 90%;
    height: 400px;
    margin-left:auto;
    margin-right:auto;
    margin-top: 10%;
}

.dragHistoryTableHeader{
	background-color: #119Dc9;
    color: #FFF;
    height: 37px;
}